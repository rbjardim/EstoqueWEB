@page
@model EstoqueWEB.Pages.Admin.UsersModel

<h1>Usuários</h1>

<table class="table">
    <thead>
        <tr>
            <th>Nome</th>
            <th>Email</th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.Users)
        {
            <tr>
                <td>@user.Nome</td>
                <td>@user.Email</td>
                <td>
                    
                </td>
            </tr>
        }
    </tbody>
</table>
